{
  "expo": {
    "name": "BR-31 Expense Suite",
    "slug": "br-31-expense-suite",
    "version": "3.1.0",
    "orientation": "default",
    "icon": "./assets/tsl_icon.png",
    "splash": {
      "image": "./assets/tsl_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "statusBar": {
        "translucent": true,
        "backgroundColor": "transparent"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/tsl_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "live.br31tech.androidtslexpense"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/tsl_icon.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}
